//perfect number 
import java.util.*;
public class HelloWorld {
    public static void main(String[] args) {
    int n=6;
    int sum=0;
    for(int i=1;i<n;i++)
    {
        if(n%i==0)
        {
        System.out.println(i);
        sum=sum+i;
        }
    }
    if(n==sum)
    {
        System.out.println("Perfect Number");
        
    }
    else
    {
        System.out.println("Not perfect number");
    }
    }
}
