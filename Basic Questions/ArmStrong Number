import java.util.*;
public class HelloWorld {
    public static void main(String[] args) {
    int n=153;
    int r,temp;
    int sum=0;
    temp=n;
    while(n>0)
    {
    r=n%10;
    n=n/10;
    sum=sum+r*r*r;
    }
    if(temp==sum)
    {
        System.out.println("Armstrong Number:"+temp);
    }
    else
    {
        System.out.println("Not Armstrong Number");
    }
    }
}
