public class HelloWorld{

     public static void main(String []args){
      int minrange=10;
      int maxrange=30;
      boolean flag=false;
      for(int n=minrange;n<=maxrange;n++)
      {
          for(int i=2;i<n;i++)
          {
              if(n%i==0)
              {
                  flag=true;
                  break;
              }
          }
          if(flag==false)
          {
              System.out.println(n);
          }
          flag=false;
      }
     }
}
