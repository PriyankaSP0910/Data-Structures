//remove vowels
import java.util.*;
public class HelloWorld{
    public static void main(String args[])
    {
    String s="priyanka";
    int l=s.length();
    String res=" ";
    for(int i=0;i<l;i++)
    {
        char z=s.charAt(i);
        if(z=='a' || z=='e' || z=='i' || z=='o' || z=='u')
        {
            continue;
        }
        else
        {
            res+=s.charAt(i);
        }
    }
    System.out.println(res);
}
}

