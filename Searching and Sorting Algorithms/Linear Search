class HelloWorld {
    public static void main(String[] args) {
       int a[]={1,2,3,4,5,6};
       int key=7;
       boolean flag=false;
       for(int i=0;i<a.length;i++)
       {
           if(key==a[i])
           {
               flag=true;
               System.out.println("Key found at index:"+i);
               break;
           }
       }
       if(flag==false)
       {
       System.out.println("Key not found");
       }
    }
}
