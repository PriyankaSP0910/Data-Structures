import java.util.*;


class HelloWorld {
    public static void main(String[] args) {
        int a[]={5,6,2,1,3};
        Arrays.sort(a);
        int i=0;
        int j=a.length-1;
        while(i<j)
        {
            System.out.println(a[j]);
            System.out.println(a[i]);
            i++;
            j--;
        }
        if(a.length%2!=0)
        {
            System.out.println(a[i]);
        }
    }
}
