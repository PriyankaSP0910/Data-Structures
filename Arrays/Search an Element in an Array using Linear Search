import java.util.*;
public class HelloWorld{
    public static void main(String args[])
    {
int a[]={1,3,4,2,5,6,8};
int s=2;
boolean t=false;
for(int i=0;i<a.length;i++)
{
    if(a[i]==2)
    {
       System.out.println("Element found at index:"+i);
        t=true;
        break;
    }
}
if(t==false)
{
    System.out.println("element not found");
}
}
}
