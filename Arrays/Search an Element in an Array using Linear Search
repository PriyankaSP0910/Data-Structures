import java.util.*;
public class HelloWorld{
    public static void main(String args[])
    {
int a[]={2,3,4,5,6,8};
int s=90;
boolean t=false;
for(int i=0;i<a.length;i++)
{
    if(s==a[i])
    {
        System.out.println("Element found at index:"+a[i]);
        t=true;
        break;
    }
    
}
if(t==false)
{
    System.out.println("element not found");
}
}
}

