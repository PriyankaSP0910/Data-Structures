import java.util.*;
public class HelloWorld{
    public static void main(String args[])
    {
int a[]={2,9,4,1};
int min=a[0],max=a[0];
for(int i=1;i<a.length;i++)
{
    if(min>a[i])
    {
        min=a[i];
    }
    if(max<a[i])
    {
        max=a[i];
    }
}
System.out.println("Maximum: "+max+" Minimum: "+min);

}
}

