class HelloWorld {
    public static void main(String[] args) {
        int a[]={16,17,4,3,5,2};
        int n=a.length;
        int max=0;
        for(int i=0;i<n;i++)
        {
            for(int j=i+1;j<n;j++)
            {
                if(a[j]>max)
                {
                    max=a[j];
                }
                
            }
            a[i]=max;
            max=-1;
        }
        for(int i=0;i<n;i++)
        {
        System.out.println(a[i]);
        }
    }
}
